{
  "name": "nativescript-circle-menu",
  "version": "1.0.0",
  "description": "CircleMenu is a simple, elegant menu with a circular layout. http://ramotion.com.",
  "main": "nativescript-circle-menu.js",
  "nativescript": {
    "platforms": {
      "ios": "2.0.1"
    }
  },
  "scripts": {
    "build": "tsc",
    "demo.ios": "npm run preparedemo && cd demo && tns emulate ios",
    "preparedemo": "npm run build && cd demo && tns plugin remove nativescript-circle-menu && tns plugin add .. && tns install",
    "setup": "cd demo && npm install && cd .. && npm run build && cd demo && tns plugin add .. && cd .."
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/DeviantJS/nativescript-circle-menu.git"
  },
  "keywords": [
    "NativeScript",
    "JavaScript",
    "iOS",
    "circle-menu",
    "menu"
  ],
  "author": {
    "name": "Nathan Walker",
    "email": "walkerrunpdx@gmail.com"
  },
  "contributors": [
    "Joshua Wiens <wiens.joshua@gmail.com> (https://github.com/d3viant0ne)"
  ],
  "bugs": {
    "url": "https://github.com/DeviantJS/nativescript-circle-menu/issues"
  },
  "license": {
    "type": "MIT",
    "url": "https://github.com/DeviantJS/nativescript-circle-menu/blob/master/LICENSE"
  },
  "homepage": "https://github.com/DeviantJS/nativescript-circle-menu",
  "readmeFilename": "README.md",
  "devDependencies": {
    "tns-platform-declarations": "^2.0.0",
    "typescript": "^1.8.10"
  }
}
